<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.PreferredFunction" table="preferred_function" schema="wyjob">
        <id name="pfunctionId">
            <column name="pfunction_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="function">
            <column name="function" sql-type="int(11)"/>
        </property>
        <many-to-one name="applicantByUserId" class="com._51job.domain.Applicant">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="dictionaryByFunction" class="com._51job.domain.Dictionary">
            <column name="function"/>
        </many-to-one>
    </class>
</hibernate-mapping>