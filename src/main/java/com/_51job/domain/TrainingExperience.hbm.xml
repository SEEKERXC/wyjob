<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.TrainingExperience" table="training_experience" schema="wyjob">
        <id name="texperienceId">
            <column name="texperience_id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="lecture">
            <column name="lecture" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="discription">
            <column name="discription" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>