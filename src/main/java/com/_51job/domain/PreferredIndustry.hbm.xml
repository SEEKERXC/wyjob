<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.PreferredIndustry" table="preferred_industry" schema="wyjob">
        <id name="pindustryId">
            <column name="pindustry_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="industry">
            <column name="industry" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>