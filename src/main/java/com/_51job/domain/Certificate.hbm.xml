<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.Certificate" table="certificate" schema="wyjob">
        <id name="certificateId">
            <column name="certificate_id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="time">
            <column name="time" sql-type="date"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="applicantByUserId" class="com._51job.domain.Applicant">
            <column name="user_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>