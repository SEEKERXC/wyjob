<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.Matrix" table="matrix" schema="wyjob">
        <id name="matrixId">
            <column name="matrix_id" sql-type="int(11)"/>
        </id>
        <property name="recruitmentId">
            <column name="recruitment_id" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="int(11)"/>
        </property>
        <property name="time">
            <column name="time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="level">
            <column name="level" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="recruitmentByRecruitmentId" class="com._51job.domain.Recruitment">
            <column name="recruitment_id"/>
        </many-to-one>
        <many-to-one name="dictionaryByName" class="com._51job.domain.Dictionary">
            <column name="name"/>
        </many-to-one>
    </class>
</hibernate-mapping>