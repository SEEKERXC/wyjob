<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.Enterprise" table="enterprise" schema="wyjob">
        <id name="enterpriseId">
            <column name="enterprise_id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="domicile">
            <column name="domicile" sql-type="int(11)"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="foundingTime">
            <column name="founding_time" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="scale">
            <column name="scale" sql-type="int(11)"/>
        </property>
        <property name="industry">
            <column name="industry" sql-type="int(11)"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)"/>
        </property>
        <property name="description">
            <column name="description" sql-type="text" not-null="true"/>
        </property>
        <property name="userName">
            <column name="user_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="userMobile">
            <column name="user_mobile" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <many-to-one name="dictionaryByIndustry" class="com._51job.domain.Dictionary">
            <column name="industry"/>
        </many-to-one>
        <many-to-one name="dictionaryByType" class="com._51job.domain.Dictionary">
            <column name="type"/>
        </many-to-one>
        <one-to-one name="userByEnterpriseId" class="com._51job.domain.User"/>
        <set name="recruitmentsByEnterpriseId" inverse="true">
            <key>
                <column name="enterprise_id"/>
            </key>
            <one-to-many not-found="ignore" class="com._51job.domain.Recruitment"/>
        </set>
    </class>
</hibernate-mapping>