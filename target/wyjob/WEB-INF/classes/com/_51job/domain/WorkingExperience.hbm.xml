<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.WorkingExperience" table="working_experience" schema="wyjob">
        <id name="wexperienceId">
            <column name="wexperience_id" sql-type="int(11)"/>
        </id>
        <property name="enterpriseName">
            <column name="enterprise_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="enterpriseType">
            <column name="enterprise_type" sql-type="int(11)"/>
        </property>
        <property name="enterpriseScale">
            <column name="enterprise_scale" sql-type="int(11)"/>
        </property>
        <property name="industry">
            <column name="industry" sql-type="int(11)"/>
        </property>
        <property name="function">
            <column name="function" sql-type="int(11)"/>
        </property>
        <property name="post">
            <column name="post" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="salary">
            <column name="salary" sql-type="int(11)"/>
        </property>
        <property name="workType">
            <column name="work_type" sql-type="tinyint(1)"/>
        </property>
        <property name="workDescription">
            <column name="work_description" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>