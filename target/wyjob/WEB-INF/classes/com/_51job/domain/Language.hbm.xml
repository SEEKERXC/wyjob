<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com._51job.domain.Language" table="language" schema="wyjob">
        <id name="languageId">
            <column name="language_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="language">
            <column name="language" sql-type="int(11)"/>
        </property>
        <property name="rwAbility">
            <column name="rw_ability" sql-type="tinyint(1)"/>
        </property>
        <property name="xAbility">
            <column name="x_ability" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>